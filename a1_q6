import java.util.Scanner;

public class Assignment1 {
    public static void main(String[] arg){
        Scanner scn = new Scanner(System.in);
        int num = scn.nextInt();
        System.out.println("Reverse of "+num+" is ");
        while (num>0){
            int r=num%10;
            num=num/10;
            System.out.print(r);
        }
    }
}

