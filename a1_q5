import java.util.Scanner;

public class Assignment1 {
    public static void main(String[] arg){
        Scanner scn = new Scanner(System.in);
        int num = scn.nextInt();
        int dig=0;
        System.out.println("Number of digits in "+num+" is ");
        while(num!=0){
            num=num/10;
            dig++;
        }
        System.out.println(dig);
        scn.close();
   }
}
